{
  "name": "algorithm-practice",
  "version": "1.0.0",
  "description": "algorithm-practice",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint:eslint": "npx eslint . --fix",
    "lint:prettier": "npx prettier . --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts}": [
      "pnpm lint:eslint",
      "pnpm lint:prettier",
      "git add"
    ]
